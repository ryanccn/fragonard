{
	"compilerOptions": {
		"strict": true,
		"strictNullChecks": true,
		"module": "esnext",
		"target": "es2019",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"noEmit": true
	},
	"exclude": ["node_modules", "**/node_modules/*", "dist", "**/dist/*"]
}
